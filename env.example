# Environment Variables Template
# Copy this file to .env and update the values as needed

NODE_ENV=development
PORT=3000
APP_NAME=GitHub Workflow NestJS API
APP_VERSION=1.0.0
API_PREFIX=api/v1

# Database (example)
DATABASE_URL=postgresql://localhost:5432/github_workflow_dev

# API Keys (example - change these in production!)
API_SECRET_KEY=dev-secret-key-12345
JWT_SECRET=dev-jwt-secret-key

# External Services
EXTERNAL_API_URL=https://api.dev.example.com
CACHE_TTL=300

# Feature Flags
ENABLE_LOGGING=true
ENABLE_CORS=true
DEBUG_MODE=true
